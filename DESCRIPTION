Package: RJDemetra
Type: Package
Title: R Interface to JDemetra+ Seasonal Adjustment Software
Version: 0.1.1
Authors@R: c(
  person("Raf", "Baeyens", role = "aut",
                email = "raf.baeyens@nbb.be"),
  person("Anna", "Michalek", role = "aut",
                email = "anka.michalek@gmail.com"),
